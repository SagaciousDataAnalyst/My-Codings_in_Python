   ],
   "source": [
    "# it peforms the OR operation on the idividual bits of the number\n",
    "\n",
    "print(10 | 11) #bin(10) = 0000 1010\n",
    "               #bin(11) = 0000 1011\n",
    "               #        = 0000 1011 = bin(11)\n",
    "\n"
   ]
