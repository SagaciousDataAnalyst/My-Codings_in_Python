   "source": [
    "def digit_sum(num):\n",
    "    original=num\n",
    "    sum = 0\n",
    "    while(num!=0):\n",
    "        digit = num%10\n",
    "        sum = sum+digit\n",
    "        num = num//10\n",
    "    print(\"Sum of digits of {} is {}: \".format(original,sum))\n",
    "digit_sum(1729)\n"
   ]
  }
