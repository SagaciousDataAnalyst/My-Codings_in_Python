    "def geometric_mean():\n",
    "    count=0\n",
    "    product=1\n",
    "    total=int(input(\"enter the no of the elements : \"))\n",
    "    for i in range(total):\n",
    "        num=float(input())\n",
    "        count+=1\n",
    "        product*=num\n",
    "    print(\"geometric mean : \",pow(product,1/count))\n",
    "\n",
    "geometric_mean()  \n",
    "    "
   ]
