   "source": [
    "# Write a program in Python to check if a number is binary?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "63888718",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "No,it is not a binary number\n",
      "Yes, it a binary number\n"
     ]
    }
   ],
   "source": [
    "def isbinary(num):\n",
    "    if(str(0) in str(num) and str(1) in str(num)):\n",
    "        return \"Yes, it a binary number\"\n",
    "    else:\n",
    "        return \"No,it is not a binary number\"\n",
    "print(isbinary(1729))\n",
    "print(isbinary(10010))"
   ]
