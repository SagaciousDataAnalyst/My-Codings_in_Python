   "source": [
    "def prime(n):\n",
    "    temp=0\n",
    "    for i in range(2,n//2):\n",
    "        if(n%i==0):\n",
    "            temp=1\n",
    "            break\n",
    "    if(temp==0):\n",
    "        return n,\"is a prime number\"\n",
    "    else:\n",
    "        return n,\"is not a prime number\"\n",
    "prime(int(input(\"Enter your number : \")))"
   ]
