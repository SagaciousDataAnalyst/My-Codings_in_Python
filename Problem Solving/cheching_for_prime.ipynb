    "def isprime():\n",
    "    temp=0\n",
    "    n = int(input(\"please give a number : \"))\n",
    "    for i in range(2,n//2):\n",
    "        if n%i == 0:\n",
    "            temp=1\n",
    "            break\n",
    "    if temp == 1:\n",
    "        return \"given number is not prime\"\n",
    "    else:\n",
    "        return \"given number is prime\" \n",
    "isprime()"
