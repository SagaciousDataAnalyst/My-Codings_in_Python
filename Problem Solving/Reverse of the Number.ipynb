    "def revere(number):\n",
    "    rev = 0\n",
    "    while(number!=0):\n",
    "        digit = number%10\n",
    "        rev = (rev*10)+digit\n",
    "        number = number//10\n",
    "    print(rev)\n",
    "revere(1729)\n",
    "\n",
    "def reverse(number):\n",
    "    print(str(number)[::-1])\n",
    "reverse(1729)\n"
   ]
  }
