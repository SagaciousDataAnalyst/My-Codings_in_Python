    "def nested_if(num):\n",
    "    if(num>0):\n",
    "        if(num%2==0):return \"It is positive even number\"\n",
    "        else:return \"It is positive odd number\"\n",
    "    else:\n",
    "        if(num%2==0):return \"It is negative even number\"\n",
    "        else:return \"It is negative odd number\"\n",
    "print(nested_if(-1728))"
   ]
