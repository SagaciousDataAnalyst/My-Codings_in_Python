   "source": [
    "from threading import *\n",
    "class Hello(Thread):\n",
    "    def run(self):\n",
    "        for i in range(10):\n",
    "            print(\"hello\")\n",
    "        \n",
    "class Hi(Thread):\n",
    "    def run(self):\n",
    "        for i in range(10):\n",
    "            print(\"hi\")\n",
    "        \n",
    "t1=Hello()\n",
    "t2=Hi()\n",
    "t1.run()\n",
    "t2.run()"
   ]
  }
