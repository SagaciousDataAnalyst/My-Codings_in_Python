    "def f(l):\n",
    "    even=[i for i in l if(i%2==0)]\n",
    "    odd=[i for i in l if(i%2==1)]\n",
    "    even.sort()\n",
    "    odd.sort()\n",
    "    even.extend(odd[::-1])\n",
    "    print(even)    \n",
    "f([2,3,52,74,6,7])"
