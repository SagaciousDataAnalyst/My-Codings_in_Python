   "source": [
    "def decor(f):     # This is the decorator function\n",
    "    def inner():  # this is the inner function that modifies\n",
    "        value=f()\n",
    "        return value+2\n",
    "    return inner  # return the inner function\n",
    "\n",
    "def num():\n",
    "    return 10\n",
    "\n",
    "num = decor(num)\n",
    "num()"
   ]
  }
